<h2>Sign up</h2>
<div class="col-md-4">
  <%= form_for(resource, as: resource_name, url: user_registration_path) do |f| %>
      <%= devise_error_messages! %>

      <div class="form-group">
        <%= f.label :name %><br />
        <%= f.text_field :name, autofocus: true, tabindex: '1', class:'form-control' %>
      </div>

      <div class="form-group">
        <%= f.label :email %><br />
        <%= f.email_field :email, tabindex: '2', class:'form-control' %>
      </div>

      <div class="form-group">
        <%= f.label :password %>
        <%= f.password_field :password, autocomplete: "off", tabindex: '3', class:'form-control' %>
      </div>

      <div class="form-group">
        <%= f.label :password_confirmation %><br />
        <%= f.password_field :password_confirmation, autocomplete: "off" , tabindex: '4', class:'form-control'%>
      </div>

      <div class="actions">
        <%= f.submit "Sign up" , class:'btn btn-default' %>
      </div>
  <% end %>
</div>
<div class="col-md-12">
  <%= render "devise/shared/links" %>
</div>
